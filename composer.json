{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "mews/captcha": "~2.0",
        "guzzlehttp/guzzle": "~4.0",
        "ssdb/phpssdb": "dev-master",
        "php": ">=5.5.9",
        "laravel/framework": "5.1.*",
        "barryvdh/laravel-debugbar": "^2.0",
        "ignited/laravel-omnipay": "2.*",
        "lokielse/omnipay-alipay": "dev-master",
        "Nathanmac/Parser": "3.*",
        "vinkla/hashids": "^2.2",
        "jenssegers/agent": "^2.3",
        "webpatser/laravel-uuid": "2.*",
        "nesbot/Carbon": "^1.21",
        "doctrine/dbal": "^2.5",
        "latrell/qqwry": "dev-master",
        "league/csv": "^8.0",
        "dingo/api": "1.0.x@dev",
        "tymon/jwt-auth": "0.5.*"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~4.0",
        "phpspec/phpspec": "~2.1"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "files": [
            "app/helpers.php"
        ],
        "psr-4": {
            "Omnipay\\Alipay\\": "custom/alipay/",
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "scripts": {},
    "config": {
        "preferred-install": "dist"
    }
}
